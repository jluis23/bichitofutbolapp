<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <LinearLayout
        android:id="@+id/home_top"
        android:layout_width="match_parent"
        android:layout_height="0dip"
        android:layout_weight=".5"
        android:background="@drawable/fondo_2_bichito">
        
        	<TableLayout
        	    android:layout_width="fill_parent"
            	android:layout_height="fill_parent">
            	
            	<TableRow
                android:id="@+id/tableRow1"
                android:layout_width="wrap_content"
                android:layout_height="0dp"
                android:layout_weight="1">

                <LinearLayout
                    android:layout_height="fill_parent"
                    android:layout_width="wrap_content"
                	android:layout_weight="1"
                	android:paddingLeft="10dp">

                   <ImageView
				        android:id="@+id/claro_bichito_home_text"
				        android:layout_height="fill_parent"
				        android:layout_width="fill_parent"
				        android:contentDescription="ImagenFondo"
				        android:src="@drawable/texto" />

                </LinearLayout>
                
                <LinearLayout
                    android:layout_height="fill_parent"
                    android:layout_width="wrap_content"
                	android:layout_weight="1"
                	android:paddingRight="5dp">

                    <ImageView
                        android:id="@+id/claro_bichito_home_image"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="bottom"
                        android:baselineAlignBottom="true"
                        android:contentDescription="ImagenFondo"
                        android:src="@drawable/bicho" />
                </LinearLayout>
            </TableRow>
        	</TableLayout>
	</LinearLayout>
  
    <LinearLayout
        android:id="@+id/home_bot"
        android:orientation="vertical" 
        android:layout_height="0dip"
        android:layout_width="match_parent"
        android:layout_weight="0.4"
        android:background="@android:color/black">
    
    <LinearLayout
        android:id="@+id/home_bot_content_table"
        android:layout_width="match_parent"
        android:paddingTop="10dp"
       	android:paddingBottom="10dp"
        android:layout_height="match_parent">
        
        <TableLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent">
            
            <TableRow
                android:id="@+id/tableRow1"
                android:layout_width="wrap_content"
                android:layout_height="0dp"
                android:layout_weight="1">

                <LinearLayout
                    android:layout_height="fill_parent"
                    android:layout_width="0dp"
                	android:layout_weight="1"
                	android:paddingLeft="40dp"
                	android:background="@drawable/noticias04">

                    <ImageButton
                        android:id="@+id/btn_noticias"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@android:color/transparent"
                        android:gravity="center"
                        android:onClick="navigate"
                        android:scaleType="center" />

                </LinearLayout>
                
                <LinearLayout
                    android:layout_height="fill_parent"
                    android:layout_width="0dp"
                	android:layout_weight="1"
                	android:paddingRight="20dp"
                	android:background="@drawable/cabina">

                    <ImageButton
                        android:id="@+id/btn_cabina_claro"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@android:color/transparent"
                        android:gravity="center"
                        android:onClick="navigate"
                        android:scaleType="center" />

                </LinearLayout>
            </TableRow>

            <TableRow
                android:id="@+id/tableRow2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                 >

                <LinearLayout
                    android:layout_height="fill_parent"
                    android:layout_width="0dp"
                	android:layout_weight="1"
                	android:paddingLeft="20dp"
                	android:background="@drawable/calendario04">
                    
                    <ImageButton
                        android:id="@+id/btn_calendario"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@android:color/transparent"
                        android:onClick="navigate"
                        android:scaleType="center" />

                </LinearLayout>
                
                 <LinearLayout
                    android:layout_height="fill_parent"
                    android:layout_width="0dp"
                	android:layout_weight="1"
                	android:paddingRight="20dp"
                	android:background="@drawable/clarotv">

                    <ImageButton
                        android:id="@+id/btn_claro_tv"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@android:color/transparent"
                        android:onClick="navigate"
                        android:scaleType="center" />

                </LinearLayout>
            </TableRow>

            <TableRow
                android:id="@+id/tableRow3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1" >

                <LinearLayout
                    android:layout_height="fill_parent"
                    android:layout_width="0dp"
                	android:layout_weight="1"
                	android:paddingLeft="20dp"
                	android:background="@drawable/tablaposiciones_04">

                    <ImageButton
                        android:id="@+id/btn_tablaposiciones"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@android:color/transparent"
                        android:onClick="navigate"
                        android:scaleType="center" />

                </LinearLayout>
                
                 <LinearLayout
                    android:layout_height="fill_parent"
                    android:layout_width="0dp"
                	android:layout_weight="1"
                	android:paddingRight="20dp"
                	android:background="@drawable/marcadorclaro04">

                     <ImageButton
                         android:id="@+id/btn_marcador"
                         android:layout_width="match_parent"
                         android:layout_height="match_parent"
                         android:background="@android:color/transparent"
                         android:onClick="navigate"
                         android:scaleType="fitCenter" />

                </LinearLayout>
            </TableRow>

            <TableRow
                android:id="@+id/tableRow4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1" >
                
                <LinearLayout
                    android:layout_height="fill_parent"
                    android:layout_width="0dp"
                	android:layout_weight="1"
                	android:paddingLeft="20dp"
                	android:background="@drawable/btn_alertas04" >

                    <ImageButton
                        android:id="@+id/btn_equipos"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@android:color/transparent"
                        android:onClick="navigate"
                        android:scaleType="center" />

                </LinearLayout>
                
                <LinearLayout
                    android:layout_height="fill_parent"
                    android:layout_width="0dp"
                	android:layout_weight="1"
                	android:paddingRight="20dp"
                	android:background="@drawable/golesfecha04" >

                    <ImageButton
                        android:id="@+id/btn_goles"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@android:color/transparent"
                        android:onClick="navigate"
                        android:scaleType="center" />

                </LinearLayout>
            </TableRow>
            
            <TableRow
                android:id="@+id/tableRow5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="5dp"
                android:layout_weight="1" >
                
                <LinearLayout
                    android:layout_height="fill_parent"
                    android:layout_width="0dp"
                	android:layout_weight="1"
                	android:paddingLeft="20dp"
                	android:background="@drawable/redessociales04" >

                    <ImageButton
                        android:id="@+id/btn_redes_sociales"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@android:color/transparent"
                        android:onClick="navigate"
                        android:scaleType="center" />

                </LinearLayout>
                
                <LinearLayout
                    android:layout_height="fill_parent"
                    android:layout_width="0dp"
                	android:layout_weight="1"
                	android:paddingRight="20dp"
                	android:background="@drawable/fotos04" >

                    <ImageButton
                        android:id="@+id/btn_fotos"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@android:color/transparent"
                        android:onClick="navigate"
                        android:scaleType="center" />

                </LinearLayout>
            </TableRow>
        </TableLayout>
    </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:id="@+id/home_ads"
        android:orientation="vertical"
        android:layout_gravity="center"
        android:gravity="center"
        android:layout_height="0dip"
        android:layout_width="match_parent"
        android:layout_weight="0.1"
        android:background="@android:color/black">
     	<RelativeLayout
                    android:id="@+id/adsdkContent12"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:layout_centerHorizontal="true"
                    android:layout_gravity="center_horizontal"
                    android:gravity="center_horizontal" >
        </RelativeLayout>
    </LinearLayout>
</LinearLayout>